programa
{
	
	funcao inicio()
	{
 
          
		cadeia nome 
		real a, b, c, media
		escreva("Informe o nome:")
		leia(nome)
		escreva("\nInforme a n1:")
		leia(a)
		escreva("\nInforme a n2:")
		leia(b)
		escreva("\nInforme a n3:")
		leia(c)
		media = 3/((1/a)+(1/b)+(1/c))

		escreva("\nA média é:", media)
	}
}
